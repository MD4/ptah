{
  "name": "ptah",
  "private": true,
  "scripts": {
    "start": "cd apps/app && pnpm start",
    "start:ui": "cd apps/ui-admin && pnpm start",
    "build": "turbo build --force --no-cache",
    "clean": "turbo clean",
    "dev": "turbo dev --no-cache  --continue --concurrency 20",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\" --config .prettier.config.js",
    "lint": "turbo lint",
    "test": "turbo test",
    "typecheck": "turbo typecheck"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "turbo": "^2.3.1"
  },
  "packageManager": "pnpm@9.12.3"
}