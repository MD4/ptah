{
  "name": "ptah",
  "private": true,
  "scripts": {
    "start": "cd apps/app && pnpm start",
    "start:ui": "cd apps/ui-admin && pnpm start",
    "build": "turbo build --force --no-cache",
    "clean": "turbo clean",
    "dev": "turbo dev --no-cache  --continue --concurrency 20",
    "format": "biome format --write",
    "check": "biome check",
    "check:fix": "biome check --write",
    "test": "turbo test",
    "typecheck": "turbo typecheck"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "turbo": "^2.3.1"
  },
  "packageManager": "pnpm@9.15.4"
}
